<UserControl x:Class="Capinfo.WPF.APP.Login"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Capinfo.WPF.APP"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button x:Name="button" Content="Login" HorizontalAlignment="Left" Margin="268,272,0,0" VerticalAlignment="Top" Width="246" Cursor="Hand"/>
        <CheckBox x:Name="checkBox" Content="Remember me" HorizontalAlignment="Left" Margin="268,234,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="268,122,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="246"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="268,181,0,0" VerticalAlignment="Top" Width="246"/>
        <Label x:Name="label" Content="ForgetPassword" HorizontalAlignment="Left" Margin="410,234,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
